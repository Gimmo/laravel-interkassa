{
    "name": "gimmo/laravel-interkassa",
    "description": "Interkassa payments for Laravel",
    "keywords": ["interkassa", "laravel"],
    "license": "MIT",
    "homepage": "https://github.com/Gimmo/laravel-interKassa",
    "authors": [
        {
            "name": "Gimmo",
            "email": "xbtc.bot@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": "8.*",
        "guzzlehttp/guzzle": "7.*"
    },
    "autoload": {
        "psr-4": {
            "Gimmo\\Interkassa\\Facades\\": "src/Facades",
            "Gimmo\\Interkassa\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gimmo\\Interkassa\\InterkassaServiceProvider"
            ],
            "aliases": {
                "Interkassa": "Gimmo\\Interkassa\\Facades\\Interkassa"
            }
        }
    }
}
